{
  "cli": {
    "version": ">= 3.12.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "cache": {
        "key": "ios-preview-{{ checksum \"package-lock.json\" }}"
      },
      "env": {
        "EXPO_USE_FAST_RESOLVER": "1",
        "NODE_OPTIONS": "--max-old-space-size=4096"
      }
    },
    "production": {
      "ios": {
        "buildConfiguration": "Release"
      },
      "cache": {
        "key": "ios-production-{{ checksum \"package-lock.json\" }}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
