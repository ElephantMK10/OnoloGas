{
  "cli": {
    "version": ">= 16.13.4",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.18.0",
      "ios": {
        "cocoapods": "1.15.2"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EAS_BUILD": "true"
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "env": {
        "EAS_BUILD": "true"
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "env": {
        "EAS_BUILD": "true"
      },
      "ios": {
        "resourceClass": "large"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
