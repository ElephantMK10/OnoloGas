{
  "name": "onolo-group-orders",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo export --platform web && cp -r public/* dist/",
    "lint": "expo lint",
    "format": "prettier --write .",
    "test": "jest",
    "clean": "rm -rf node_modules .expo ios android .metro-cache && npm cache clean --force",
    "clean:install": "npm run clean && npm install",
    "prebuild:clean": "expo prebuild --clear",
    "build:ios": "EAS_BUILD=true eas build --platform ios --profile production",
    "build:ios:fast": "EXPO_USE_FAST_RESOLVER=1 NODE_OPTIONS='--max-old-space-size=4096' expo export --platform ios",
    "optimize:images": "npx expo-optimize",
    "analyze:bundle": "npx expo export --platform ios --output-dir dist-analyze && npx source-map-explorer dist-analyze/_expo/static/js/ios/*.js",
    "cache:clear": "rm -rf .metro-cache $TMPDIR/metro-* $TMPDIR/haste-map-*"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.4.1",
    "@react-native/virtualized-lists": "^0.80.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.18.1",
    "crypto-js": "^4.2.0",
    "expo": "53.0.20",
    "expo-auth-session": "~6.2.0",
    "expo-constants": "~17.1.3",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.2.2",
    "expo-web-browser": "^14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-flow": "^7.27.1",
    "@expo/metro-config": "^0.20.17",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "~19.0.10",
    "@types/uuid": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "~29.7.0",
    "jest-expo": "^53.0.9",
    "prettier": "^3.2.5",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
